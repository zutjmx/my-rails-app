<div id="<%= dom_id friend %>">
  <p>
    <strong>Nombre:</strong>
    <%= friend.first_name %>
  </p>

  <p>
    <strong>Apellidos:</strong>
    <%= friend.last_name %>
  </p>

  <p>
    <strong>Correo Electrónico:</strong>
    <%= friend.email %>
  </p>

  <p>
    <strong>Teléfono:</strong>
    <%= friend.phone %>
  </p>

  <p>
    <strong>Cuenta de Twitter:</strong>
    <%= friend.twitter %>
  </p>

</div>
